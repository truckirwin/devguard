     ],
    "keybindings": [
      {
        "command": "theWritersRoom.openAIChat",
        "key": "ctrl+shift+a",
        "mac": "cmd+shift+a"
      },
      {
        "command": "theWritersRoom.createProject",
        "key": "ctrl+shift+n",
        "mac": "cmd+shift+n"
      },

      {
        "command": "theWritersRoom.resetTreeViews",
        "key": "ctrl+shift+r",
        "mac": "cmd+shift+r"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "theWritersRoom.createProject",
          "when": "true"
        },
        {
          "command": "theWritersRoom.openAIChat",
          "when": "true"
        },

        {
          "command": "theWritersRoom.openSettings",
          "when": "true"
        },
        {
          "command": "theWritersRoom.resetTreeViews",
          "when": "true"
        },
        {
          "command": "theWritersRoom.refreshTreeViews",
          "when": "true"
        },
        {
          "command": "theWritersRoom.configureAgentModels",
          "when": "true"
        },
        {
          "command": "theWritersRoom.openSettingsWebview",
          "when": "true"
        }
      ],
      "view/title": [
        {
          "command": "theWritersRoom.selectAgent",
          "when": "view == theWritersRoom.agentExplorer",
          "group": "navigation"
        },
        {
          "command": "theWritersRoom.refreshTreeViews",
          "when": "view == theWritersRoom.agentExplorer",
          "group": "navigation",
          "title": "Refresh"
        },
        {
          "command": "theWritersRoom.resetTreeViews",
          "when": "view == theWritersRoom.agentExplorer",
          "group": "navigation",
          "title": "Reset"
        },
        {
          "command": "theWritersRoom.configureAgentModels",
          "when": "view == theWritersRoom.agentExplorer",
          "group": "navigation",
          "title": "Configure Models"
        },
        {
          "command": "theWritersRoom.openSettings",
          "when": "view == theWritersRoom.agentExplorer",
          "group": "navigation",
          "title": "Settings"
        },
        {
          "command": "theWritersRoom.openSettingsWebview",
          "when": "view == theWritersRoom.agentExplorer",
          "group": "navigation",
          "title": "Settings Panel"
        },
        {
          "command": "theWritersRoom.refreshTreeViews",
          "when": "view == theWritersRoom.projectExplorer",
          "group": "navigation",
          "title": "Refresh"
        },
        {
          "command": "theWritersRoom.resetTreeViews",
          "when": "view == theWritersRoom.projectExplorer",
          "group": "navigation",
          "title": "Reset"
        },
        {
          "command": "theWritersRoom.createProject",
          "when": "view == theWritersRoom.projectExplorer",
          "group": "navigation",
          "title": "New Project"
        }
      ],
      "view/item/context": [
        {
          "command": "theWritersRoom.refreshTreeViews",
          "when": "view == theWritersRoom.agentExplorer",
          "group": "inline"
        },
        {
          "command": "theWritersRoom.resetTreeViews",
          "when": "view == theWritersRoom.agentExplorer",
          "group": "inline"
        },
        {
          "command": "theWritersRoom.refreshTreeViews",
          "when": "view == theWritersRoom.projectExplorer",
          "group": "inline"
        },
        {
          "command": "theWritersRoom.resetTreeViews",
          "when": "view == theWritersRoom.projectExplorer",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package-extension",
    "compile": "NODE_OPTIONS='--no-warnings' tsc -p ./",
    "compile-tests": "NODE_OPTIONS='--no-warnings' tsc -p ./ --outDir out",
    "watch": "NODE_OPTIONS='--no-warnings' tsc -watch -p ./",
    "package-extension": "webpack --mode production --devtool hidden-source-map",
    "compile-web": "webpack",
    "watch-web": "webpack --watch",
